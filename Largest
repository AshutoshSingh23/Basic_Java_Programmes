import java.util.Scanner;
public class Main {
    public static void main(String[] args){
        int a,b,c,m;
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter three numbers:");
        a=sc.nextInt();
        b=sc.nextInt();
        c=sc.nextInt();
        m=(a>b)?((a>c)?(a):(c)):((b>c)?(b):(c));
        System.out.println("Largest number is "+m);
    }
}

