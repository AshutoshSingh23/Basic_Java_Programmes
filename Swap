import java.util.Scanner;
public class Swap1 {
    public static void main(String[] args){
        int a,b,c;
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter numbers:");
        a=sc.nextInt();
        b=sc.nextInt();
        c=b;
        b=a;
        a=c;
        System.out.println(a);
        System.out.println(b);
    }
}

import java.util.Scanner;
public class Swap2 {
    public static void main(String[] args){
        int a,b,c;
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter numbers:");
        a=sc.nextInt();
        b=sc.nextInt();
        a=a+b;
        b=a-b;
        a=a-b;
        System.out.println(a);
        System.out.println(b);
    }
}
